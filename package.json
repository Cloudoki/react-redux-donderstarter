{
  "name": "react-redux-donderstarter",
  "version": "1.0.0",
  "description": "react, redux, react-router boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "mock-api": "json-server --watch mock-api/db.json --port 9004",
    "prebuild": "jest && rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "test": "jest --watch",
    "prestart": "yarn build",
    "start": "http-server-spa ./dist index.html 8080"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudoki/react-redux-donderstarter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "boilerplate"
  ],
  "contributors": [
    "muffles <molefas@gmail.com>",
    "mstrk <mstrk512@gmail.com> (http://www.meetbruno.eu/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cloudoki/react-redux-donderstarter/issues"
  },
  "homepage": "https://github.com/Cloudoki/react-redux-donderstarter.git#readme",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "history": "4.7.2",
    "immutability-helper": "2.5.0",
    "js-cookie": "2.2.0",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-intl": "2.4.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "cross-env": "5.1.1",
    "css-hot-loader": "1.3.3",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.11.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "http-server-spa": "1.2.0",
    "jest": "21.2.1",
    "json-server": "0.12.1",
    "node-sass": "4.6.1",
    "postcss-loader": "2.0.8",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.1.1",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-merge": "4.1.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "es2015": {
            "loose": true,
            "modules": false
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "prefer-const": 2,
      "no-var": 2,
      "prefer-arrow-callback": 2,
      "arrow-body-style": 2,
      "react/no-unused-prop-types": 0,
      "import/no-unresolved": 2
    },
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "jest": {
    "setupFiles": [
      "./internals/jest/shim.js",
      "./internals/jest/setup.js"
    ],
    "modulePaths": [
      "src"
    ]
  }
}
