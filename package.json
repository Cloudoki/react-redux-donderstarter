{
  "name": "react-redux-donderstarter",
  "version": "1.0.0",
  "description": "react, redux, react-router boilerplate",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "mock-api": "json-server --watch __mocks__/db.json --port 9004",
    "prebuild": "yarn test && rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "pretest": "yarn lint && yarn check-types",
    "test": "cross-env NODE_ENV=test jest --c ./jest.json --coverage",
    "commit": "git-cz",
    "prestart": "yarn build",
    "start": "serve -s dist -l 8080",
    "open-cov": "open coverage/lcov-report/index.html",
    "prerelease": "git fetch --tags",
    "release": "standard-version",
    "check-types": "tsc",
    "lint": "eslint './src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudoki/react-redux-donderstarter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "boilerplate"
  ],
  "contributors": [
    "muffles <molefas@gmail.com>",
    "mstrk <mstrk512@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cloudoki/react-redux-donderstarter/issues"
  },
  "homepage": "https://github.com/Cloudoki/react-redux-donderstarter.git#readme",
  "dependencies": {
    "clsx": "1.0.4",
    "connected-react-router": "6.5.2",
    "history": "4.9.0",
    "immutability-helper": "3.0.1",
    "js-cookie": "2.2.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "redux": "4.0.4",
    "redux-saga": "1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
