{
  "presets": [
    ["@babel/preset-env", {
      "corejs": 3,
      "useBuiltIns": "usage",
      "loose": true,
      "modules": "commonjs",
      "forceAllTransforms": true
    }],
    "@babel/preset-react",
    "@babel/typescript",
  ],
  "env": {
    "dev": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "presets": [["@babel/env"]],
      "plugins": ["@babel/plugin-transform-runtime"],
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
  ]
}
